{
    "name": "private-packagist/vendor-data-exporter",
    "description": "A tool to fetch package versions that a vendor's customers have access to.",
    "keywords": ["packagist", "composer", "api", "sdk", "vendor", "customer"],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PrivatePackagist\\VendorDataExporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zan Baldwin",
            "email": "z.baldwin@packagist.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.3"
    },
    "require": {
        "composer/semver": "^3.3",
        "guzzlehttp/guzzle": "^7.4",
        "php-http/guzzle7-adapter": "^1.0",
        "private-packagist/api-client": "^1.28",
        "symfony/console": "^6.1",
        "symfony/dotenv": "^6.1"
    }
}
